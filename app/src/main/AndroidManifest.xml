<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bangkit.idku.instances">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".IDKuInstances"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.IndonesiaKuInstances">
        <activity android:name=".request_result.RequestPermissionResultActivity"/>
        <activity
            android:name=".login.LoginActivity"
            android:theme="@style/Theme.IndonesiaKuInstances.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".home.HomeActivity" />
    </application>

</manifest>